<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <listener>
        <listener-class>com.lab8.servlets.ExpandedServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>SingersServlet</servlet-name>
        <servlet-class>com.lab8.servlets.SingerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SingersServlet</servlet-name>
        <url-pattern>/singers</url-pattern>
    </servlet-mapping>

</web-app>